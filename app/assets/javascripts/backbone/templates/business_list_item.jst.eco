<a id='business-list-item' data-marker-id='<%=@_leaflet_id%>'> <%= @feature.properties.name %></a>
  <% if @feature.properties.business.get('food'):%> <i class='fa fa-cutlery'></i><% end %>
  <% if @feature.properties.business.get('license'):%> <i class='fa fa-beer'></i><% end %>
  <% if @feature.properties.business.get('hot_drinks'):%> <i class='fa fa-coffee'></i><% end %>